from typing import Any, IO, Tuple

class JSONDecodeError(object):
    def dumps(self, obj: Any) -> str: ...
    def dump(self, obj: Any, fp: IO[str], *args: Any, **kwds: Any) -> None: ...
    def loads(self, s: str) -> Any: ...
    def load(self, fp: IO[str]) -> Any: ...

def dumps(obj, skipkeys=False, ensure_ascii=True, check_circular=True, allow_nan=True, indent: int = None, separators: Tuple[str, str] = None, encoding='utf-8', sort_keys=False) -> str: ...
def dump(obj, fp, skipkeys=False, ensure_ascii=True, check_circular=True, allow_nan=True, indent: int = None, separators: Tuple[str, str] = None, encoding='utf-8', sort_keys=False) -> None: ...
def loads(s: str) -> Any: ...
def load(fp: IO[str]) -> Any: ...
